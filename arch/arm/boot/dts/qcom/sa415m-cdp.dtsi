#include "sdxpoorwills-cdp.dtsi"

&vbus_detect {
	status = "okay";
};

&qcom_seecom {
	status = "okay";
};

&usb {
	status = "okay";
	qcom,connector-type-uAB;
	extcon = <0>, <0>, <0>, <&vbus_detect>;
	dwc3@a600000 {
		normal-eps-in-gsi-mode;
	};
};

&smb138x {
	status = "disabled";
};

&thermal_zones {
	mdm-core-step {
		trips {
			mdm-step-trip-0 {
				temperature = <105000>;
			};

			mdm-step-trip-1 {
				temperature = <115000>;
			};
		};
	};
};
