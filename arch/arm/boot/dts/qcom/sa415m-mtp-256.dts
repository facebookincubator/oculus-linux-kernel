/dts-v1/;

#include "sdxpoorwills-mtp-256.dtsi"

/ {
	model = "Qualcomm Technologies, Inc. SA415M MTP (256MB)";
	compatible = "qcom,sdxpoorwills-mtp",
		"qcom,sdxpoorwills", "qcom,mtp";
	qcom,board-id = <8 0x106>;
};

&blsp1_uart2b_hs {
	status = "okay";
};

&mss_mem {
	reg = <0x86400000 0x9300000>;
};

&smb138x {
	status = "disabled";
};

&thermal_zones {
	mdm-core-step {
		trips {
			mdm-step-trip-0 {
				temperature = <105000>;
			};

			mdm-step-trip-1 {
				temperature = <115000>;
			};
		};
	};
};
