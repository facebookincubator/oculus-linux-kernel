&rpm_bus {
	rpm-regulator-smpa2 {
		status = "okay";
		mdm9607_s2: regulator-s2 {
			compatible = "qcom,rpm-smd-regulator";
			regulator-name = "mdm9607_s2";
			regulator-min-microvolt = <750000>;
			regulator-max-microvolt = <1275000>;
			qcom,init-voltage = <750000>;
			status = "okay";
		};
	};

