&pm8994_mpps {
	mpp@a300 { /* MPP 4 */
		qcom,mode = <4>;		/* A-input */
		qcom,invert = <1>;		/* Enable MPP*/
		qcom,src-sel = <0>;		/* Constant */
		qcom,ain-route = <3>;	/*QPNP_PIN_AIN_AMUX_CH8 = 3 for MPP4*/
		qcom,master-en = <1>;	/* Enable GPIO */
		status = "okay";
	};

	mpp@a400 { /* MPP 5 */
		qcom,mode = <1>;		/* Digital output */
		qcom,output-type = <0>;	/* CMOS logic */
		qcom,vin-sel = <1>;		/* ldo19 3V */
		qcom,src-sel = <0>;		/* Constant */
		qcom,master-en = <1>;	/* Enable GPIO */
		qcom,invert = <1>;
		status = "okay";
	};

	mpp@a600 { /* MPP 7 */
		qcom,mode = <0>;		/* QPNP_PIN_MODE_DIG_IN */
		qcom,vin-sel = <2>;		/* S4 1.8V */
		qcom,src-sel = <1>;		/* QPNP_PIN_SEL_FUNC_PAIRED*/
		qcom,master-en = <1>;	/* Enable GPIO */
		status = "okay";
	};

	mpp@a700 {
		qcom,mode = <1>;		/* QPNP_PIN_MODE_DIG_OUT */
		qcom,vin-sel = <1>;		/* LDO19*/
		qcom,src-sel = <1>;		/* QPNP_PIN_SEL_FUNC_PAIRED*/
		qcom,master-en = <1>;	/* Enable GPIO */
		status = "okay";
	};
};

&qusb_phy0 {
	reg = <0x07411000 0x180>,
		<0x06af8800 0x400>,
		<0x00388018 0x4>;
	reg-names = "qusb_phy_base",
		"qscratch_base",
		"ref_clk_addr";
	qcom,qusb-phy-init-seq = <0xF8 0x80
		0x63 0x84
		0x83 0x88
		0xC0 0x8C
		0x30 0x08
		0x79 0x0C
		0x21 0x10
		0x14 0x9C
		0x9F 0x1C
		0x00 0x18>;
};

&pm8994_gpios {
	gpio@c900 {
		qcom,mode = <0>;
		qcom,pull = <0>;
		qcom,vin-sel = <2>;
		qcom,src-sel = <0>;
		status = "okay";
	};

	gpio@ca00 {
		qcom,mode = <0>;
		qcom,pull = <0>;
		qcom,vin-sel = <2>;
		qcom,src-sel = <0>;
		status = "ok";
