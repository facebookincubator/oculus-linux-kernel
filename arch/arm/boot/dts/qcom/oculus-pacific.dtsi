&tlmm {
	spk_l_ext_pa {
		spk_l_sleep: spk_l_sleep {
			mux {
				pins = "gpio59";
				function = "gpio";
			};
			config {
				pins = "gpio59";
				drive-strength = <16>;
				bias-disable;
				output-low;
			};
		};
		spk_l_active: spk_l_active {
			mux {
				pins = "gpio59";
				function = "gpio";
			};
			config {
				pins = "gpio59";
				drive-strength = <16>;
				bias-pull-down;
				output-high;
			};
		};
	};

	spk_r_ext_pa {
		spk_r_sleep: spk_r_sleep {
			mux {
				pins = "gpio60";
				function = "gpio";
			};
			config {
				pins = "gpio60";
				drive-strength = <16>;
				bias-disable;
				output-low;
			};
		};
		spk_r_active: spk_r_active {
			mux {
				pins = "gpio60";
				function = "gpio";
			};
			config {
				pins = "gpio60";
				drive-strength = <16>;
				bias-pull-down;
				output-high;
			};
		};
	};

	gpio_led_white {
		gpio_led_white_close: gpio_led_white_close {
			mux {
				pins = "gpio16";
				function = "gpio";
			};
			config {
				pins = "gpio16";
				drive-strength = <2>;
				bias-pull-down;
				output-high;
			};
		};
		gpio_led_white_open: gpio_led_white_open {
			mux {
				pins = "gpio16";
				function = "gpio";
			};
			config {
				pins = "gpio16";
				drive-strength = <2>;
				bias-pull-up;
				output-high;
			};
		};
	};

	gpio_led_black {
		gpio_led_black_close: gpio_led_black_close {
			mux {
				pins = "gpio17";
				function = "gpio";
			};
			config {
				pins = "gpio17";
				drive-strength = <2>;
				bias-pull-down;
				output-high;
			};
		};
		gpio_led_black_open: gpio_led_black_open {
			mux {
				pins = "gpio17";
				function = "gpio";
			};
			config {
				pins = "gpio17";
				drive-strength = <2>;
				bias-pull-up;
				output-high;
			};
		};
	};

	spi_5 {
		spi_5_active: spi_5_active {
			mux {
				pins = "gpio81", "gpio82", "gpio84";
				function = "blsp_spi5";
			};

			config {
				pins = "gpio81", "gpio82", "gpio84";
				drive-strength = <6>;
				bias-disable;
			};
		};

		spi_5_sleep: spi_5_sleep {
			mux {
				pins = "gpio81", "gpio82", "gpio84";
				function = "blsp_spi5";
			};

			config {
				pins = "gpio81", "gpio82", "gpio84";
				drive-strength = <6>;
				bias-disable;
			};
		};

		spi_5_irq_active: spi_5_irq_active {
			mux {
				pins = "gpio115";
				function = "gpio";
			};
			config {
				pins = "gpio115";
				bias-pull-down;
				input-enable;
			};
		};

		spi_5_irq_sleep: spi_5_irq_sleep {
			mux {
				pins = "gpio115";
				function = "gpio";
			};
			config {
				pins = "gpio115";
				bias-pull-down;
				input-enable;
			};
		};

		spi_5_cs_active: spi_5_cs_active {
			mux {
				pins = "gpio83";
				function = "gpio";
			};
			config {
				pins = "gpio83";
				drive-strength = <6>;
				bias-disable;
				output-high;
			};
		};

		spi_5_cs_sleep: spi_5_cs_sleep {
			mux {
				pins = "gpio83";
				function = "gpio";
			};
			config {
				pins = "gpio83";
				drive-strength = <6>;
				bias-disable;
				output-high;
			};
		};
	};
};

