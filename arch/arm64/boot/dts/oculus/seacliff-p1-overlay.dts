/dts-v1/;
/plugin/;

#include <dt-bindings/clock/qcom,gcc-kona.h>
#include <dt-bindings/clock/qcom,camcc-kona.h>
#include <dt-bindings/clock/qcom,rpmh.h>
#include <dt-bindings/interrupt-controller/arm-gic.h>

#include "seacliff.dtsi"
#include "seacliff-bcmdhd.dtsi"

/ {
	model = "Seacliff (P1)";
	compatible = "qcom,kona-mtp", "qcom,kona", "qcom,mtp";
	qcom,board-id = <8 62>;
};

&cam_cci0 {
	qcom,cam-sensor2 {
		csiphy-sd-index = <0>;
	};
	qcom,cam-sensor4 {
		csiphy-sd-index = <5>;
	};
	qcom,cam-sensor5 {
		csiphy-sd-index = <3>;
	};
};

&projector {
	status = "disabled";
};

&oculusnrf {
	oculus,syncboss-has-prox;

	swd@0 {
		oculus,fw-path = "syncboss_P1_BOARD.bin";
	};
};
