#include <dt-bindings/clock/qcom,camcc-kona.h>

&cam_sensor_mclk5_active {
	config {
		drive-strength = <6>; /* 6 MA */
	};
};

&cam_sensor_mclk5_suspend {
	config {
		drive-strength = <6>; /* 6 MA */
	};
};

&pm8150a_l2 {
	/* Teton power */
	regulator-always-on;
	regulator-min-microvolt = <1050000>;
	regulator-max-microvolt = <1050000>;
	qcom,init-voltage = <1050000>;
	qcom,init-mode = <4>; /* RPMH_REGULATOR_MODE_HPM */
};

&pm8150a_l3 {
	/* Glacier power */
	regulator-always-on;
	qcom,init-voltage = <1200000>;
	qcom,init-mode = <4>; /* RPMH_REGULATOR_MODE_HPM */
};

&pm8150a_l10 {
	/* Canyon, Glacier, Teton power */
	regulator-always-on;
	regulator-min-microvolt = <2960000>;
	regulator-max-microvolt = <2960000>;
	qcom,init-voltage = <2960000>;
	qcom,init-mode = <4>; /* RPMH_REGULATOR_MODE_HPM */
};

&pm8150_l15 {
	/* Canyon, Glacier, Teton, power */
	regulator-always-on;
	qcom,init-mode = <4>; /* RPMH_REGULATOR_MODE_HPM */
};

&soc {
	qcom,cam-cdm-intf {
		cdm-client-names = "vfe", "vife", "jpegdma", "jpegenc";
	};

	qcom,cam-res-mgr {
		compatible = "qcom,cam-res-mgr";
		status = "ok";
	};
};

&cam_cci0 {
	pinctrl-0 = <&cci0_active>;
	pinctrl-1 = <&cci0_suspend>;
	gpios = <&tlmm 101 0>,
		<&tlmm 102 0>;
	gpio-req-tbl-num = <0 1>;
	gpio-req-tbl-flags = <1 1>;
	gpio-req-tbl-label = "CCI_I2C_DATA0",
		"CCI_I2C_CLK0";

	qcom,cam-sensor0 {
		/* CAMERA ID = 0
		 * name: CAM_OG_LEFT
		 * QCOM CSI PHY = 2
		 * QCOM MCLK = 2
		 */
		cell-index = <0>;
		compatible = "qcom,cam-sensor";
		csiphy-sd-index = <2>;
		/* T97436334: Regulators are intentionally misnamed */
		cam_vana-supply = <&pm8150a_l10>;
		cam_vio-supply = <&pm8150_l15>;
		cam_vdig-supply = <&pm8150a_l3>;
		cam_clk-supply = <&titan_top_gdsc>;
		regulator-names = "cam_vana", "cam_vio", "cam_vdig",
			"cam_clk";
		rgltr-cntrl-support;
		pwm-switch;
		rgltr-min-voltage = <2960000 1800000 1200000 0>;
		rgltr-max-voltage = <2960000 1800000 1200000 0>;
		rgltr-load-current = <80000 120000 1200000 0>;
		gpio-no-mux = <0>;
		pinctrl-names = "cam_default", "cam_suspend";
		pinctrl-0 = <&cam_sensor_mclk2_active>;
		pinctrl-1 = <&cam_sensor_mclk2_suspend>;
		gpios = <&tlmm 96 0>;
		gpio-req-tbl-num = <0>;
		gpio-req-tbl-flags = <1>;
		gpio-req-tbl-label = "CAMIF_MCLK2";
		sensor-mode = <0>;
		cci-master = <1>;
		clocks = <&clock_camcc CAM_CC_MCLK2_CLK>;
		clock-names = "cam_clk";
		clock-cntl-level = "turbo";
		clock-rates = <19200000>;
		oculus,cam-mclk-no-power-down;
	};

	qcom,cam-sensor1 {
		/* CAMERA ID = 1
		 * name: CAM_OG_RIGHT
		 * QCOM CSI PHY = 3
		 * QCOM MCLK = 3
		 */
		cell-index = <1>;
