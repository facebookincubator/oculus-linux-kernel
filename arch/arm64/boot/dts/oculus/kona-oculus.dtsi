#include <dt-bindings/gpio/gpio.h>
#include <dt-bindings/input/input.h>

&flash_led {
	status = "disabled";
};

&pm8150b_haptics {
	status = "disabled";
};

&qcom_seecom {
	reg = <0x82400000 0x1C00000>;
};

&removed_mem {
	reg = <0x0 0x80b00000 0x0 0x3500000>;
};

&reserved_memory {
	ramoops: ramoops {
		compatible = "ramoops";
		reg = <0 0x9BA00000 0 0x00090000>;
		record-size = <131072>;
		console-size = <131072>;
		pmsg-size = <131072>;
	};
};

&sde_dsi {
	qcom,platform-te-gpio = <>;
};

&sde_dsi1 {
	qcom,platform-te-gpio = <>;
};

&soc {
	gpio_keys {
		compatible = "gpio-keys";
		label = "gpio-keys";

		pinctrl-names = "default";
		pinctrl-0 = <&key_vol_up_default>;

		vol_up {
			label = "volume_up";
			gpios = <&pm8150_gpios 6 GPIO_ACTIVE_LOW>;
			linux,input-type = <1>;
			linux,code = <KEY_VOLUMEUP>;
			gpio-key,wakeup;
			debounce-interval = <15>;
			linux,can-disable;
		};
	};
};

&wlan {
	reg = <0xb0000000 0x0>;
	reg-names = "";
};
