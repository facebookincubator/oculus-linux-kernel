#include <dt-bindings/gpio/gpio.h>
#include <dt-bindings/input/input.h>

#include "default-sim-panels.dtsi"

&cam_fd {
	status = "disabled";
};

&ipa_hw {
	status = "disabled";
};

&mhi_netdev_0 {
	status = "disabled";
};

&mhi_netdev_0_rsc {
	status = "disabled";
};

&pil_slpi_mem {
	status = "disabled";
};

&dump_mem {
	status = "disabled";
};

&flash_led {
	status = "disabled";
};

&i3c0 {
	status = "disabled";
};

&i3c1 {
	status = "disabled";
};

&mdm0 {
	status = "disabled";
};

&pcie1 {
	status = "disabled";
};

&pcie2 {
	status = "disabled";
};

&pm8150b_haptics {
	status = "disabled";
};

&qcom_seecom {
	reg = <0x82400000 0x1C00000>;
};

&qupv3_se4_i2c {
	status = "ok";
	qcom,clk-freq-out = <100000>;

	bq27z561: fuel-gauge@55 {
		compatible = "ti,bq27z561";
		reg = <0x55>;
		status = "okay";
	};
};

&disp_rdump_memory {
	status = "disabled";
};

&removed_mem {
	reg = <0x0 0x80b00000 0x0 0x3500000>;
};

&reserved_memory {
	ramoops: ramoops {
		compatible = "ramoops";
		reg = <0 0x9BA00000 0 0x00090000>;
		record-size = <131072>;
		console-size = <131072>;
		pmsg-size = <131072>;
	};
};

&sde_dsi {
	qcom,platform-te-gpio = <>;
};

&sde_dsi1 {
	qcom,platform-te-gpio = <>;
};

&soc {
	gpio_keys {
		compatible = "gpio-keys";
		label = "gpio-keys";

		pinctrl-names = "default";
		pinctrl-0 = <&key_vol_up_default>;

		vol_up {
			label = "volume_up";
			gpios = <&pm8150_gpios 6 GPIO_ACTIVE_LOW>;
			linux,input-type = <1>;
			linux,code = <KEY_VOLUMEUP>;
			gpio-key,wakeup;
			debounce-interval = <15>;
			linux,can-disable;
		};
	};

	mem_dump {
		status = "disabled";
	};

	qcom,cam-fd {
		status = "disabled";
	};

	qcom,cam_smmu {
		msm_cam_smmu_fd {
			status = "disabled";
		};
	};

	qcom,ipa_fws {
		status = "disabled";
	};

	qcom,ipa-mpm {
		status = "disabled";
	};

	qcom,ipa_uc {
		status = "disabled";
	};

	qcom,msm_gsi {
		status = "disabled";
	};

	qcom,rmnet-ipa {
		status = "disabled";
	};

	qcom,ssc@5c00000 {
		status = "disabled";
	};

	usb_audio_qmi_dev {
		status = "disabled";
	};

	usb_emu_phy@a720000 {
		status = "disabled";
	};

	usb_nop_phy {
		status = "disabled";
	};
};

&spmi_debug_bus {
	status = "ok";
};

&ssc_sensors {
	status = "disabled";
};

&vendor {
	extcon_usb1: extcon_usb1 {
		compatible = "linux,extcon-usb-gpio";
		vbus-gpio = <&pm8150_gpios 10 GPIO_ACTIVE_HIGH>;
		id-gpio = <&tlmm 91 GPIO_ACTIVE_HIGH>;
		vbus-out-gpio = <&pm8150_gpios 9 GPIO_ACTIVE_HIGH>;

		pinctrl-names = "default";
		pinctrl-0 = <&usb2_vbus_det_default
			&usb2_id_det_default
			&usb2_vbus_boost_default>;
	};
};

&usb1 {
	extcon = <&extcon_usb1>;
};

&wlan {
	reg = <0xb0000000 0x0>;
	reg-names = "";
};
