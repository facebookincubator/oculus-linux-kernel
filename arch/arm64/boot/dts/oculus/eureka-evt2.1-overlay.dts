/dts-v1/;
/plugin/;

/ {
	model = "Eureka (EVT2.1)";
	compatible = "qcom,anorak", "oculus,eureka", "oculus,eureka-evt2.1";
	qcom,msm-id = <549 0x10000>;
	qcom,board-id = <0x20140 0x2>;
};

&cypd3177 {
	cypd3177,custom-sink-caps = <5000 2330>, /* 5V @ 2.33A */
	<5000 2600>,  /*  5V @ 2.60A */
	<5000 3000>;  /*  5V @ 3.00A */
};

&fan {
	oculus,max-pwm = <179>; /* 70% duty cycle */
};

&wlan {
	cal-file-prefix = "eureka_evt2.1_";
};
