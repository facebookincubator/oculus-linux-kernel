&thermal_zones {
	batt-usr {
		polling-delay-passive = <100>;
		polling-delay = <1000>;
		thermal-governor = "user_space";
		thermal-sensors = <&pm8150b_adc_tm ADC_BAT_THERM_PU2>;
		wake-capable-sensor;
		trips {
			batt_trip: batt-trip {
				temperature = <600>;
				hysteresis = <10>;
				type = "passive";
			};
		};
	};

	cam0-usr {
		/* Glacier left */
		polling-delay-passive = <0>;
		polling-delay = <0>;
		thermal-governor = "user_space";
		thermal-sensors = <&pm8150l_adc_tm ADC_AMUX_THM1_PU2>;
		trips {
