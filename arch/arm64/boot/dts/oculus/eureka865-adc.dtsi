&pm8150_adc_tm {
	#address-cells = <1>;
	#size-cells = <0>;

	io-channels = <&pm8150_vadc ADC_AMUX_THM1_PU2>,
		<&pm8150_vadc ADC_AMUX_THM2_PU2>,
		<&pm8150_vadc ADC_GPIO1_PU2>;

	pinctrl-names = "default";
	pinctrl-0 = <&pm_gpio_adc_default>;

	imu_therm@4d {
		reg = <ADC_AMUX_THM1_PU2>;
		qcom,ratiometric;
		qcom,hw-settle-time = <200>;
	};

	rfpa_therm1@4e {
		reg = <ADC_AMUX_THM2_PU2>;
		qcom,ratiometric;
		qcom,hw-settle-time = <200>;
	};

	cam4_therm@52 {
		/* RGB camera */
		reg = <ADC_GPIO1_PU2>;
		qcom,ratiometric;
		qcom,hw-settle-time = <200>;
	};
};

&pm8150_gpios {
	pm8150_adc_therm {
		pm_gpio_adc_default: pm_gpio_adc_default {
			pins = "gpio3";
			function = "normal";
			bias-high-impedance;
		};
	};
};

&pm8150_vadc {
	#address-cells = <1>;
	#size-cells = <0>;

	imu_therm@4d {
		reg = <ADC_AMUX_THM1_PU2>;
		label = "imu_therm";
		qcom,ratiometric;
		qcom,hw-settle-time = <200>;
		qcom,pre-scaling = <1 1>;
	};

	rfpa_therm1@4e {
		reg = <ADC_AMUX_THM2_PU2>;
		label = "rfpa_therm";
		qcom,ratiometric;
		qcom,hw-settle-time = <200>;
		qcom,pre-scaling = <1 1>;
	};

	cam4_therm@52 {
		/* RGB camera */
		reg = <ADC_GPIO1_PU2>;
		label = "cam4_therm";
		qcom,ratiometric;
		qcom,hw-settle-time = <200>;
		qcom,pre-scaling = <1 1>;
	};
};

