			batt_fan_trip0: batt_fan_trip0 {
				temperature = <40000>;
				hysteresis = <2000>;
				type = "active";
			};

			batt_fan_trip1: batt_fan_trip1 {
				temperature = <48000>;
				hysteresis = <2000>;
