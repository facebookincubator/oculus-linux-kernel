		wake-capable-sensor;
		trips {
			cam0_usr_fan_trip0: cam0_usr_fan_trip0 {
				temperature = <40000>;
				hysteresis = <3000>;
				type = "active";
			};

			cam0_usr_fan_trip1: cam0_usr_fan_trip1 {
				temperature = <55000>;
				hysteresis = <3000>;
				type = "active";
