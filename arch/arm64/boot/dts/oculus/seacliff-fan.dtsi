			tp_fan_1_map0: tp_fan_1_map0 {
				trip = <&tp_fan_trip0>;
				/* RPM: 0 - 1800 */
				cooling-device = <&fan_1 0 2>;
			};

			tp_fan_1_map1: tp_fan_1_map1 {
				trip = <&tp_fan_trip1>;
				/* RPM: 1800 - 2200 */
				cooling-device = <&fan_1 2 4>;
			};

			tp_fan_1_map2: tp_fan_1_map2 {
				trip = <&tp_fan_trip2>;
				/* RPM: 2200 - 2400 */
				cooling-device = <&fan_1 4 5>;
			};

			tp_fan_1_map3: tp_fan_1_map3 {
				trip = <&tp_fan_trip3>;
				/* RPM: 2400 - 2600 */
				cooling-device = <&fan_1 5 6>;
			};
		};
	};
};
