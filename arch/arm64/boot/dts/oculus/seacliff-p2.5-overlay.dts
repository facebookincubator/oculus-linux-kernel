/dts-v1/;
/plugin/;

#include "seacliff-non-pop.dtsi"

/ {
	model = "Seacliff (P2.5)";
	compatible = "qcom,kona";
	qcom,board-id = <66 66>;
};

&pm8150_l10 {
	/* Esker power */
	regulator-always-on;
};

&pm8150_l15 {
	/* Canyon, Glacier, Teton, Esker power */
	regulator-always-on;
};

&pm8150a_l1 {
	/* IPD 1.8 */
	regulator-always-on;
};

&pm8150a_l2 {
	/* Teton power */
	regulator-always-on;
};

&pm8150a_l3 {
	/* Glacier power, FPGA power */
	regulator-always-on;
};

&pm8150a_l10 {
	/* Canyon, Glacier, Teton power */
	regulator-always-on;
};

&qupv3_se0_spi {
	oculusnrf@0 {
		oculus,syncboss-has-no-prox-cal;

		swd@0 {
			oculus,fw-path = "syncboss-ipl-spl.bin";
		};
	};
};

&pm8150_adc_tm {
	imu_therm@4d {
		reg = <ADC_AMUX_THM1_PU2>;
		qcom,ratiometric;
		qcom,hw-settle-time = <200>;
	};
};

&pm8150_vadc {
	imu_therm@4d {
		reg = <ADC_AMUX_THM1_PU2>;
		label = "imu_therm";
		qcom,ratiometric;
		qcom,hw-settle-time = <200>;
		qcom,pre-scaling = <1 1>;
	};
};

&thermal_zones {
	imu-usr {
		polling-delay-passive = <0>;
		polling-delay = <0>;
		thermal-governor = "user_space";
		thermal-sensors = <&pm8150_adc_tm ADC_AMUX_THM1_PU2>;
		trips {
			active-config0 {
				temperature = <125000>;
				hysteresis = <1000>;
				type = "passive";
			};
		};
	};
};
