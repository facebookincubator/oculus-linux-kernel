/dts-v1/;
/plugin/;

#include "eureka865.dtsi"

/ {
	model = "Eureka865 (Dev0)";
	compatible = "qcom,kona";
	qcom,board-id = <68 144>;
};

&pm8150_adc_tm {
	io-channels = <&pm8150_vadc ADC_XO_THERM_PU2>,
		<&pm8150_vadc ADC_AMUX_THM1_PU2>,
		<&pm8150_vadc ADC_AMUX_THM2_PU2>,
		<&pm8150_vadc ADC_GPIO1_PU2>;

	imu_therm@4d {
		reg = <ADC_AMUX_THM1_PU2>;
		qcom,ratiometric;
		qcom,hw-settle-time = <200>;
	};
};

&pm8150_vadc {
	imu_therm@4d {
		reg = <ADC_AMUX_THM1_PU2>;
		label = "imu_therm";
		qcom,ratiometric;
		qcom,hw-settle-time = <200>;
		qcom,pre-scaling = <1 1>;
	};
};

&pm8150b_adc_tm {
	io-channels = <&pm8150b_vadc ADC_AMUX_THM2_PU2>,
		<&pm8150b_vadc ADC_AMUX_THM4_PU2>,
		<&pm8150b_vadc ADC_BAT_THERM_PU2>,
		<&pm8150b_vadc ADC_GPIO3_PU2>;

	smb_therm@4e {
		reg = <ADC_AMUX_THM2_PU2>;
		qcom,ratiometric;
		qcom,hw-settle-time = <200>;
	};

	cam3_therm@54 {
		/* Canyon right */
		reg = <ADC_GPIO3_PU2>;
		qcom,ratiometric;
		qcom,hw-settle-time = <200>;
	};
};

&pm8150l_rgb_led {
	green {
		pwms = <&pm8150l_lpg 2 1000000>;
		led-sources = <2>;
	};

	blue {
		pwms = <&pm8150l_lpg 1 1000000>;
		led-sources = <1>;
	};
};

&thermal_zones {
	cam3-usr {
		/* Canyon right */
		polling-delay-passive = <0>;
		polling-delay = <0>;
		thermal-governor = "user_space";
		thermal-sensors = <&pm8150b_adc_tm ADC_GPIO3_PU2>;
		trips {
			active-config0 {
				temperature = <125000>;
				hysteresis = <1000>;
				type = "passive";
			};
		};
	};

	imu-usr {
		polling-delay-passive = <0>;
		polling-delay = <0>;
		thermal-governor = "user_space";
		thermal-sensors = <&pm8150_adc_tm ADC_AMUX_THM1_PU2>;
		trips {
			active-config0 {
				temperature = <125000>;
				hysteresis = <1000>;
				type = "passive";
			};
		};
	};

	smb-usr {
		polling-delay-passive = <0>;
		polling-delay = <0>;
		thermal-governor = "user_space";
		thermal-sensors = <&pm8150b_adc_tm ADC_AMUX_THM2_PU2>;
		trips {
			active-config0 {
				temperature = <125000>;
				hysteresis = <1000>;
				type = "passive";
			};
		};
	};
};
