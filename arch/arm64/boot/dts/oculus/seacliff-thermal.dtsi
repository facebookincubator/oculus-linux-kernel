&mdss_mdp {
	#cooling-cells = <2>;
};

&vendor {
	battery_virtual_sensor: battery_virtual_sensor {
		compatible = "oculus,virtual-sensor";
		status = "ok";
		#thermal-sensor-cells = <0>;

		fallback-thermal-zone = "battery";
		fallback-tolerance = <10000>;

		thermal-zones = "battery";
		thermal-zone-scaling-factors = <1>;

		internal-polling-delay = <1000>;

		coefficients {
			charging {
				tz-coefficients = <5061>;
				tz-slope-coefficients = <500>;
				intercept = <14511>;
			};
			discharging {
				tz-coefficients = <5061>;
				tz-slope-coefficients = <500>;
				intercept = <14511>;
			};
		};
	};

	display_virtual_sensor_fan: display_virtual_sensor_fan {
		compatible = "oculus,virtual-sensor";
		status = "ok";
		#thermal-sensor-cells = <0>;

		io-channels = <&pm8150b_vadc ADC_AMUX_THM4_PU2>;
		io-channel-names = "pcb1_therm";

		thermal-zones = "rfpa-usr", "cpu-1-4-usr", "npu-usr",
				"smb-usr", "pogo-charger";

		internal-polling-delay = <1000>;

		coefficients {
			charging {
				tz-coefficients = <633 255 304 (-376) 2302>;
				tz-slope-coefficients = <500 (-85) (-8500) (-5500) 1500>;
				io-coefficients = <775>;
				io-slope-coefficients = <(-15500)>;
				intercept = <19771>;
			};
			discharging {
				tz-coefficients = <633 255 304 (-376) 2302>;
				tz-slope-coefficients = <500 (-85) (-8500) (-5500) 1500>;
				io-coefficients = <775>;
				io-slope-coefficients = <(-15500)>;
				intercept = <19771>;
			};
		};
	};

	display_virtual_sensor_usr: display_virtual_sensor_usr {
