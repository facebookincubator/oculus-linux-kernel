&thermal_zones {
	batt-virt {
		polling-delay-passive = <0>;
		polling-delay = <0>;
		thermal-governor = "user_space";
		thermal-sensors = <&battery_virtual_sensor>;
		wake-capable-sensor;
		trips {
			batt_virt_trip0: batt-virt-trip0 {
				temperature = <58500>;
				hysteresis = <2000>;
				type = "passive";
			};
			batt_virt_trip1: batt-virt-trip1 {
				temperature = <60000>;
				hysteresis = <1000>;
				type = "passive";
			};
		};
	};

	cpu-1-0-limit {
		polling-delay-passive = <0>;
		polling-delay = <0>;
		thermal-sensors = <&tsens0 7>;
		thermal-governor = "single_step";
		wake-capable-sensor;
		trips {
			cpus_1_0_limit_trip0: cpus_1_0_limit_trip0 {
				temperature = <90000>;
				hysteresis = <5000>;
				type = "passive";
			};
		};
		cooling-maps {
			cpu_1_0_limit_map0: cpu_1_0_limit_map0 {
