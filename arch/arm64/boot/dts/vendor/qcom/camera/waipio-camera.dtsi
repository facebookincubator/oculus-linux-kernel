&tlmm {
	cci0_active: cci0_active {
		mux {
			/* CLK, DATA */
			pins = "gpio110","gpio111"; // Only 2
			function = "cci_i2c";
		};

		config {
			pins = "gpio110","gpio111";
			bias-pull-up; /* PULL UP*/
			drive-strength = <2>; /* 2 MA */
		};
	};

	cci0_suspend: cci0_suspend {
		mux {
			/* CLK, DATA */
			pins = "gpio110","gpio111";
			function = "cci_i2c";
		};

		config {
			pins = "gpio110","gpio111";
			bias-pull-down; /* PULL DOWN */
			drive-strength = <2>; /* 2 MA */
		};
	};

