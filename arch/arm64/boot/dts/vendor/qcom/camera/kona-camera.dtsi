		regulator-names = "gdscr", "refgen";
		csi-vdd-voltage = <1200000>;
		mipi-csi-vdd-supply = <&pm8150_l9>;
		clocks = <&clock_camcc CAM_CC_CPHY_RX_CLK_SRC>,
