#include <dt-bindings/thermal/thermal_qti.h>

&thermal_zones {
	pm6150l-bcl-lvl0 {
		cooling-maps {
			vph_lte0 {
				trip = <&l_bcl_lvl0>;
				cooling-device = <&modem_lte_dsc 8 8>;
			};

			vph_nr0_scg {
