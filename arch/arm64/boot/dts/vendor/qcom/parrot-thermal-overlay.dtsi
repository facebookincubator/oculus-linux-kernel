			};

			vph_gpu1 {
				trip = <&l_bcl_lvl1>;
				cooling-device = <&msm_gpu 2 2>;
			};
		};
	};

	pm6150l-bcl-lvl2 {
		cooling-maps {
			vph_cdsp2 {
