		};
	};

	cx-pe {
		polling-delay-passive = <0>;
		polling-delay = <0>;
		thermal-sensors = <&cx_pe>;

		trips {
			cx_pe_config1: cx-pe-config1 {
				temperature = <1>;
				hysteresis = <1>;
				type = "passive";
			};
		};

		cooling-maps {
			 gpu_cdev {
				trip = <&cx_pe_config1>;
				cooling-device = <&msm_gpu 3 3>;
			};

		};
	};
};
