		};

		cooling-maps {
			gpu_cdev0 {
				trip = <&gpu_step_trip>;
				cooling-device =
					<&msm_gpu THERMAL_NO_LIMIT
						THERMAL_NO_LIMIT>;
			};
		};
	};

