&sde_dsi {
	pinctrl-names = "panel_active", "panel_suspend", "pwm_pin";
	pinctrl-2 = <&lcd_backlight_ctrl_default>;
	avdd-supply = <&display_panel_avdd>;
	qcom,avdd-regulator-gpio = <&tlmm 168 0>;
	extvdd-supply = <&display_panel_extvdd>;
	qcom,dsi-default-panel = <&dsi_nt36672e_fhd_plus_120_video>;
};

&sde_dsi1 {
	extvdd-supply = <&display_panel_extvdd>;
	qcom,avdd-regulator-gpio = <&tlmm 148 0>;
};

&qupv3_se0_i2c {
	novatek@62 {
		panel = <&dsi_nt36672e_fhd_plus_144_video
			&dsi_nt36672e_fhd_plus_120_video
			&dsi_nt36672e_fhd_plus_90hz_video
			&dsi_nt36672e_fhd_plus_60_video
			&dsi_nt36672e_dsc_fhd_plus_60hz_video
			&dsi_nt36672e_fhd_plus_144hz_video_cphy>;
	};

	atmel_mxt_ts@4a {
		panel = <&dsi_sharp_qhd_plus_dsc_cmd>;
	};

	synaptics_dsx@22 {
		panel = <&dsi_sharp_qsync_wqhd_cmd
			&dsi_sharp_qsync_wqhd_video>;
	};

	focaltech@38 {
		panel = <&dsi_r66451_amoled_60hz_video
			&dsi_r66451_amoled_60hz_cmd>;
	};
};
